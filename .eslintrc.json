{
    "env": {
      "node": true,
      "commonjs": true,
      "es2021": true,
      "jest": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "no-console": "off",
      "indent": ["error", 2],
      "semi": ["error", "always"],
      "quotes": ["error", "single"]
    }
  }
  